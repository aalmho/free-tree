      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.2100appiness.freetree"
    },
    "plugins": ["expo-apple-authentication"],
    "extra": {
      "eas": {
        "projectId": "c129b2d0-14ba-423a-8d31-d44217c8759a"
      }
    },
    "owner": "2100appiness"
  }
}
